    "aws-sdk": "^2.1004.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "camelcase-keys": "^7.0.1",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "country-code-lookup": "0.0.19",
    "country-region-data": "^1.11.0",
    "currency-formatter": "^1.5.9",
    "dayjs": "^1.10.7",
    "disposable-email-provider-domains": "^1.0.9",
    "ejs": "^3.1.6",
    "email-templates": "^8.0.8",
    "env-cmd": "^10.1.0",
