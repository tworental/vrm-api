{
  "name": "tworentals-api",
  "version": "0.0.1",
  "description": "TwoRentals Back-End API",
  "main": "index.js",
  "homepage": "https://github.com/tworentals/vrm-api#README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tworentals/vrm-api.git"
  },
  "bugs": {
    "url": "https://github.com/tworentals/vrm-api/issues"
  },
  "scripts": {
    "start": "node bin/server",
    "task": "env-cmd --silent bin/task",
    "dev": "env-cmd nodemon bin/server --inspect=0.0.0.0:9229",
    "workers:queueSubscriber": "node bin/workers",
    "workers:debug:queueSubscriber": "env-cmd nodemon bin/workers --inspect=0.0.0.0:9229",
    "test": "NODE_ENV=test jest",
    "test:integration": "NODE_ENV=test TZ=UTC env-cmd jest --maxWorkers=4 --coverage=false --testMatch='**/*.ispec.js'",
    "knex": "env-cmd --silent knex",
    "swagger": "swagger-jsdoc",
    "lint": "eslint src"
  },
  "keywords": [
    "tworentals",
    "api",
    "backend",
    "v1"
  ],
  "author": "TwoRentals",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
