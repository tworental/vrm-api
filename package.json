    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^4.3.8",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "^6.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm test"
    }
  }
